[mysql]

[mysqld]
log_bin_trust_function_creators=1
sql-mode="NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES"
character-set-server=UTF8MB4
default-authentication-plugin=mysql_native_password